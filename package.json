{
  "name": "amiif-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "docker:build": "docker build -t amiif-app . --no-cache",
    "docker:run": "docker run -p 3000:3000 amiif-app",
    "deploy": "yarn gcloud:build && yarn gcloud:deploy",
    "gcloud:build": "gcloud builds submit --tag gcr.io/anet-amiif/amiif-app --project anet-amiif",
    "gcloud:deploy": "gcloud run deploy --image gcr.io/anet-amiif/amiif-app --project anet-amiif --platform managed"
  },
  "peerDependencies": {
    "@chakra-ui/system": "2.2.7"
  },
  "dependencies": {
    "@chakra-ui/react": "2.2.9",
    "@emotion/react": "11.10.0",
    "@emotion/styled": "11.10.0",
    "@google-cloud/storage": "^6.4.1",
    "cookie": "0.5.0",
    "crypto-js": "4.1.1",
    "date-fns": "2.29.2",
    "formidable": "^2.0.1",
    "framer-motion": "7.2.1",
    "jsonwebtoken": "8.5.1",
    "mongodb": "4.9.0",
    "next": "12.2.5",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-calendar": "3.7.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.34.2",
    "react-icons": "4.4.0"
  },
  "devDependencies": {
    "eslint": "8.23.0",
    "eslint-config-next": "12.2.5",
    "eslint-config-prettier": "^8.5.0",
    "standard": "^17.0.0"
  }
}
